{
    "version": 2,
    "name": "server",
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node-server"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "app.js"
      }
    ], 
    "env": {
      "mongoURI":"my-mongoDB-secret",
      "CORS_ORIGIN": "*"
    }
  }